#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../@cloudflare/next-on-pages/node_modules/workerd/bin/workerd" "$@"
  ret=$?
else 
  node  "$basedir/../@cloudflare/next-on-pages/node_modules/workerd/bin/workerd" "$@"
  ret=$?
fi
exit $ret
